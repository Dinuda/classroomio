#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/DELL/Desktop/classroomio/classroomio/node_modules/.pnpm/ts-jest@29.1.1_@babel+core@7.22.9_babel-jest@29.7.0_jest@29.7.0_typescript@5.1.6/node_modules/ts-jest/node_modules:/mnt/c/Users/DELL/Desktop/classroomio/classroomio/node_modules/.pnpm/ts-jest@29.1.1_@babel+core@7.22.9_babel-jest@29.7.0_jest@29.7.0_typescript@5.1.6/node_modules:/mnt/c/Users/DELL/Desktop/classroomio/classroomio/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/DELL/Desktop/classroomio/classroomio/node_modules/.pnpm/ts-jest@29.1.1_@babel+core@7.22.9_babel-jest@29.7.0_jest@29.7.0_typescript@5.1.6/node_modules/ts-jest/node_modules:/mnt/c/Users/DELL/Desktop/classroomio/classroomio/node_modules/.pnpm/ts-jest@29.1.1_@babel+core@7.22.9_babel-jest@29.7.0_jest@29.7.0_typescript@5.1.6/node_modules:/mnt/c/Users/DELL/Desktop/classroomio/classroomio/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../ts-jest/cli.js" "$@"
fi
